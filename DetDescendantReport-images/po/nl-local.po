msgid "Whether to start a new page after each generation."
msgstr "Of er na elke generatie een nieuwe pagina moet worden gestart."

#: DetDescendantReport-images/detdescendantreporti.py:1123
msgid "Page break before end notes"
msgstr "Pagina-einde voor de laatste notities"

#: DetDescendantReport-images/detdescendantreporti.py:1125
msgid "Whether to start a new page before the end notes."
msgstr "Of een nieuwe pagina moet worden gestart vóór de laatste notities."

#: DetDescendantReport-images/detdescendantreporti.py:1132
msgid "Content"
msgstr "Inhoud"

#: DetDescendantReport-images/detdescendantreporti.py:1134
msgid "Use callname for common name"
msgstr "Gebruik roepnaam als algemene naam"

#: DetDescendantReport-images/detdescendantreporti.py:1135
msgid "Whether to use the call name as the first name."
msgstr "Of de roepnaam als voornaam moet worden gebruikt."

#: DetDescendantReport-images/detdescendantreporti.py:1138
msgid "Use full dates instead of only the year"
msgstr "Gebruik volledige datums in plaats van alleen het jaar"

#: DetDescendantReport-images/detdescendantreporti.py:1140
msgid "Whether to use full dates instead of just year."
msgstr "Of volledige datums gebruikt moeten worden in plaats van alleen het jaar."

#: DetDescendantReport-images/detdescendantreporti.py:1143
msgid "List children"
msgstr "Vermeld kinderen"

#: DetDescendantReport-images/detdescendantreporti.py:1144
msgid "Whether to list children."
msgstr "Of kinderen moeten worden vermeld."

#: DetDescendantReport-images/detdescendantreporti.py:1147
msgid "Compute death age"
msgstr "Bereken de leeftijd bij overlijden"

#: DetDescendantReport-images/detdescendantreporti.py:1148
msgid "Whether to compute a person's age at death."
msgstr "Of de leeftijd van een persoon bij overlijden moet worden berekend."

#: DetDescendantReport-images/detdescendantreporti.py:1151
msgid "Omit duplicate ancestors"
msgstr "Laat dubbele voorouders weg"

#: DetDescendantReport-images/detdescendantreporti.py:1152
msgid "Whether to omit duplicate ancestors."
msgstr "Of dubbele voorouders moeten worden weggelaten."

#: DetDescendantReport-images/detdescendantreporti.py:1155
msgid "Use complete sentences"
msgstr "Gebruik volledige zinnen"

#: DetDescendantReport-images/detdescendantreporti.py:1157
msgid "Whether to use complete sentences or succinct language."
msgstr "Of volledige zinnen of beknopte taal gebruikt moet worden."

#: DetDescendantReport-images/detdescendantreporti.py:1160
msgid "Add descendant reference in child list"
msgstr "Voeg afstammingsreferentie toe in de kinderlijst"

#: DetDescendantReport-images/detdescendantreporti.py:1163
msgid "Whether to add descendant references in child list."
msgstr "Of afstammingsreferenties moeten worden toegevoegd aan de kinderlijst."

