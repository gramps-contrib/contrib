#: FamilyTree/FamilyTree.gpr.py:3
msgid "Family Tree"
msgstr "Árvore genealógica"

#: FamilyTree/FamilyTree.gpr.py:4
msgid "Produces a graphical family tree."
msgstr "Gera uma árvore genealógica gráfica"

#: FamilyTree/FamilyTree.py:90
#, python-format
msgid "Family Tree for %s"
msgstr "Arvore genealógica para %s"

#: FamilyTree/FamilyTree.py:625 FamilyTree/FamilyTree.py:630
msgid "Unknown"
msgstr "Desconhecido"

#: FamilyTree/FamilyTree.py:632
#, python-format
msgid "%(father)s and %(mother)s"
msgstr "%(father)s e %(mother)s"

#: FamilyTree/FamilyTree.py:640
msgid "Anonymous"
msgstr "Anônimo"

#: FamilyTree/FamilyTree.py:837
msgid "born"
msgstr "nascido(a)"

#: FamilyTree/FamilyTree.py:839
msgid "baptised"
msgstr "batisado(a)"

#: FamilyTree/FamilyTree.py:841
msgid "died"
msgstr "falecido(a)"

#: FamilyTree/FamilyTree.py:843
msgid "buried"
msgstr "sepultado(a)"

#: FamilyTree/FamilyTree.py:845
msgid "cremated"
msgstr "cremado(a)"

#: FamilyTree/FamilyTree.py:847
msgid "married"
msgstr "casado(a)"

#: FamilyTree/FamilyTree.py:849
msgid "divorced"
msgstr "divorciado(a)"

#: FamilyTree/FamilyTree.py:851
msgid "resident"
msgstr "residência"

#: FamilyTree/FamilyTree.py:858
#, python-format
msgid "on %(ymd_date)s"
msgstr "em %(ymd_date)s"

#: FamilyTree/FamilyTree.py:860
#, python-format
msgid "in %(ym_date)s"
msgstr "em %(ym_date)s"

#: FamilyTree/FamilyTree.py:862
#, python-format
msgid "in %(y_date)s"
msgstr "em %(y_date)s"

#: FamilyTree/FamilyTree.py:866
#, python-format
msgid "on %(placeholder)s"
msgstr "em %(placeholder)s"

#: FamilyTree/FamilyTree.py:869
#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "em %(placeholder)s (%(partial)s)"

#: FamilyTree/FamilyTree.py:886
#, python-format
msgid "in %(place)s"
msgstr "em %(place)s"

#: FamilyTree/FamilyTree.py:897
#, python-format
msgid "(%(description)s)"
msgstr "(%(description)s)"

#: FamilyTree/FamilyTree.py:975
msgid "Tree Options"
msgstr "Opções da árvore"

#: FamilyTree/FamilyTree.py:977
msgid "Center Family"
msgstr "Família principal"

#: FamilyTree/FamilyTree.py:978
msgid "The center family for the tree"
msgstr "A família principal para o relatório"

#: FamilyTree/FamilyTree.py:981
msgid "Ancestor Generations"
msgstr "Número máximo de gerações de ascendentes"

#: FamilyTree/FamilyTree.py:982
msgid "The number of ancestor generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

#: FamilyTree/FamilyTree.py:985
msgid "Descendant Generations"
msgstr "Número máximo de gerações de descendentes"

#: FamilyTree/FamilyTree.py:986
msgid "The number of descendant generations to include in the tree"
msgstr "Número de gerações a serem incluídas na árvore"

#: FamilyTree/FamilyTree.py:989
msgid "Sc_ale to fit on a single page"
msgstr "Ajustar a árvore para o tamanho da página"

#: FamilyTree/FamilyTree.py:990
msgid "Whether to scale to fit on a single page."
msgstr "Ajustar para caber em uma única página"

#: FamilyTree/FamilyTree.py:993
msgid "Color"
msgstr "Cor definida por"

#: FamilyTree/FamilyTree.py:995
msgid "No color"
msgstr "Sem cor"

#: FamilyTree/FamilyTree.py:996
msgid "Generations"
msgstr "Gerações"

#: FamilyTree/FamilyTree.py:997
msgid "First generation"
msgstr "Primeira geração"

#: FamilyTree/FamilyTree.py:998
msgid "Second generation"
msgstr "Segunda geração"

#: FamilyTree/FamilyTree.py:999
msgid "Third generation"
msgstr "Terceira geração"

#: FamilyTree/FamilyTree.py:1000
msgid "Male line"
msgstr "Linhagem masculina"

#: FamilyTree/FamilyTree.py:1001
msgid "Male line and illegitimate children"
msgstr "Linhagem masculina e filhos ilegitimos"

#: FamilyTree/FamilyTree.py:1002
msgid "Female line"
msgstr "Linhagem feminina"

#: FamilyTree/FamilyTree.py:1005
msgid "Shuffle colors"
msgstr "Embaralhar cores"

#: FamilyTree/FamilyTree.py:1006
msgid "Whether to shuffle colors or order them in rainbow fashion."
msgstr "Embaralhar as cores ou ordenar como arco-iris"

#: FamilyTree/FamilyTree.py:1009
msgid "Content"
msgstr "Conteúdo"

#: FamilyTree/FamilyTree.py:1011
msgid "Use call name"
msgstr "Usar nome vocativo"

#: FamilyTree/FamilyTree.py:1013
msgid "Don't use call name"
msgstr "Não usar nome vocativo"

#: FamilyTree/FamilyTree.py:1014
msgid "Replace first name with call name"
msgstr "Substituir o primeiro nome pelo nome vocativo"

#: FamilyTree/FamilyTree.py:1015
msgid "Underline call name in first name / add call name to first name"
msgstr "Sublinhar o nome vocativo no primeiro nome / adicionar nome vocativo ao primeiro nome"

#: FamilyTree/FamilyTree.py:1018
msgid "Include Occupation"
msgstr "Incluir profissão"

#: FamilyTree/FamilyTree.py:1021
msgid "Include Residence"
msgstr "Incluir residência"

#: FamilyTree/FamilyTree.py:1024
msgid "Print event data (dead person)"
msgstr "Imprimir dados de evento (pessoa falecida)"

#: FamilyTree/FamilyTree.py:1026 FamilyTree/FamilyTree.py:1034
msgid "None"
msgstr "Nenhum(a)"

#: FamilyTree/FamilyTree.py:1027 FamilyTree/FamilyTree.py:1035
msgid "Year only"
msgstr "Somente ano"

#: FamilyTree/FamilyTree.py:1028 FamilyTree/FamilyTree.py:1036
msgid "Full date"
msgstr "Data completa"

#: FamilyTree/FamilyTree.py:1029 FamilyTree/FamilyTree.py:1037
msgid "Full date and place"
msgstr "Data e local completo"

#: FamilyTree/FamilyTree.py:1032
msgid "Print event data (living person)"
msgstr "Imprimir dados de evento (pessoa viva)"

#: FamilyTree/FamilyTree.py:1040
msgid "Fall back to baptism if birth event missing"
msgstr "Exibir evento de batismo se nascimento faltando"

#: FamilyTree/FamilyTree.py:1043
msgid "Fall back to burial or cremation if death event missing"
msgstr "Exibir evento de sepultamento ou cremação se falecimento faltando"

#: FamilyTree/FamilyTree.py:1048
msgid "Print fields for missing information"
msgstr "Imprime campos em branco para as informações ausentes"

#: FamilyTree/FamilyTree.py:1049
msgid "Whether to include fields for missing information."
msgstr "Incluir campos em branco para as informações ausentes."

#: FamilyTree/FamilyTree.py:1052
msgid "Include event description"
msgstr "Incluir descrição de eventos"

#: FamilyTree/FamilyTree.py:1055
msgid "Text Options"
msgstr "Opções de texto"

#: FamilyTree/FamilyTree.py:1057
msgid "Title text"
msgstr "Texto do título"

#: FamilyTree/FamilyTree.py:1060
msgid "Footer text"
msgstr "Texto do rodapé"

#: FamilyTree/FamilyTree.py:1075
msgid "The style used for the title."
msgstr "O estilo usado para o título."

#: FamilyTree/FamilyTree.py:1083
msgid "The style used for names."
msgstr "O estilo usado para nomes."

#: FamilyTree/FamilyTree.py:1091
msgid "The style used for data (birth, death, marriage, divorce)."
msgstr "O estilo usado para dados (nascimento, falecimento, casamento, divórcio."

#: FamilyTree/FamilyTree.py:1101
msgid "The style used for the footer."
msgstr "O estilo usado para o rodapé."
