#: FamilyTree/FamilyTree.gpr.py:3
msgid "Family Tree"
msgstr "Familiestamboom"

#: FamilyTree/FamilyTree.gpr.py:4
msgid "Produces a graphical family tree."
msgstr "Grafische familiestamboom wordt aangemaakt."

#: FamilyTree/FamilyTree.py:90
#, python-format
msgid "Family Tree for %s"
msgstr "Familistamboom voor %s"

#: FamilyTree/FamilyTree.py:625
#: FamilyTree/FamilyTree.py:630
msgid "Unknown"
msgstr "Onbekend"

#: FamilyTree/FamilyTree.py:632
#, python-format
msgid "%(father)s and %(mother)s"
msgstr "%(father)s en %(mother)s"

#: FamilyTree/FamilyTree.py:640
msgid "Anonymous"
msgstr "Anoniem"

#: FamilyTree/FamilyTree.py:837
msgid "born"
msgstr "geboren"

#: FamilyTree/FamilyTree.py:839
msgid "baptised"
msgstr "gedoopt"

#: FamilyTree/FamilyTree.py:841
msgid "died"
msgstr "gestorven"

#: FamilyTree/FamilyTree.py:843
msgid "buried"
msgstr "begraven"

#: FamilyTree/FamilyTree.py:845
