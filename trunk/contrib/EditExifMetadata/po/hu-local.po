msgstr "Leírás: "

#: EditExifMetadata/editexifmetadata.py:1025
msgid "Artist: "
msgstr "Művész: "

#: EditExifMetadata/editexifmetadata.py:1026
msgid "Copyright: "
msgstr "Szerzői jog: "

#: EditExifMetadata/editexifmetadata.py:1039
msgid "Date/ Time"
msgstr "Dátum/idő"

#: EditExifMetadata/editexifmetadata.py:1053
