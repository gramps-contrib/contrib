msgid "48.21\"S, -18.2412 or -18:9:48.21)"
msgstr "48.21\"Z, -18.2412 of -18:9:48.21)"

#: ImageMetadataGramplet/ImageMetadataGramplet.py:692
msgid "Invalid longitude (syntax: 18째9'"
msgstr "Ongeldige lengtegraad (syntax: 18째9'"

#: ImageMetadataGramplet/ImageMetadataGramplet.py:693
msgid "48.21\"E, -18.2412 or -18:9:48.21)"
msgstr "48.21\"O, -18.2412 of -18:9:48.21)"
