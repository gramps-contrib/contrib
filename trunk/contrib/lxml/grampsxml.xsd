   <sequence>
    <choice minOccurs='0' maxOccurs='1'>
     <element ref='t:daterange'/>
     <element ref='t:datespan'/>
     <element ref='t:dateval'/>
     <element ref='t:datestr'/>
    </choice>
    <element ref='t:page' minOccurs='0' maxOccurs='1'/>
    <element ref='t:confidence' minOccurs='0' maxOccurs='1'/>
    <element ref='t:noteref' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:objref' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:data_item' minOccurs='0' maxOccurs='unbounded'/>
