#, python-format
msgid "%s: "
msgstr "%s: "

#: FamilySheet/FamilySheet.py:542
#, python-format
msgid ", page %s"
msgstr ", blad %s"

#: FamilySheet/FamilySheet.py:555
msgid "Notes:"
msgstr "Opmerkingen:"

