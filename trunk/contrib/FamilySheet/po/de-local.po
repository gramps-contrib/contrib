#: FamilySheet/FamilySheet.gpr.py:25
msgid "Family Sheet"
msgstr "Familienblatt"

#: FamilySheet/FamilySheet.gpr.py:26
msgid ""
"Produces a family sheet showing full information about a person and his/her "
"partners and children."
msgstr ""
"Erstellt ein Familienblatt, das vollständige Informationen über eine Person "
"und ihre Partner und Kinder enthält."

#: FamilySheet/FamilySheet.py:210 FamilySheet/FamilySheet.py:253
#, python-format
msgid "→ %s"
msgstr "→ %s"

#: FamilySheet/FamilySheet.py:374
#, python-format
msgid "Address (%(date)s): %(location)s"
msgstr "Adresse (%(date)s): %(location)s"

#: FamilySheet/FamilySheet.py:378
#, python-format
msgid "Address: %(location)s"
msgstr "Adresse: %(location)s"

#: FamilySheet/FamilySheet.py:425
#, python-format
msgid "; %(type)s: %(value)s"
msgstr "; %(type)s: %(value)s"

#: FamilySheet/FamilySheet.py:522
msgid "Source references:"
msgstr "Quellenangaben:"

#: FamilySheet/FamilySheet.py:535
#, python-format
msgid "%s: "
msgstr "%s: "

#: FamilySheet/FamilySheet.py:539
#, python-format
msgid ", page %s"
msgstr ", Seite %s"

#: FamilySheet/FamilySheet.py:552
msgid "Notes:"
msgstr "Notizen:"

#: FamilySheet/FamilySheet.py:731
#, python-format
msgid "on %(ymd_date)s"
msgstr "am %(ymd_date)s"

#: FamilySheet/FamilySheet.py:733
#, python-format
msgid "in %(ym_date)s"
msgstr "im %(ym_date)s"

#: FamilySheet/FamilySheet.py:735
#, python-format
msgid "in %(y_date)s"
msgstr "im Jahre %(y_date)s"

#: FamilySheet/FamilySheet.py:739
#, python-format
msgid "on %(placeholder)s"
msgstr "am %(placeholder)s"

#: FamilySheet/FamilySheet.py:741
#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "am %(placeholder)s (%(partial)s)"

#: FamilySheet/FamilySheet.py:784 FamilySheet/FamilySheet.py:786
#, python-format
msgid "in %(place)s"
msgstr "in %(place)s"

#: FamilySheet/FamilySheet.py:814
msgid "Report Options"
msgstr "Berichtsoptionen"

#: FamilySheet/FamilySheet.py:817
msgid "Center person"
msgstr "Hauptperson"

#: FamilySheet/FamilySheet.py:818
msgid "The person whose partners and children are printed"
msgstr "Die Person, deren Partner und Kinder gedruckt werden"

#: FamilySheet/FamilySheet.py:821
msgid "Print sheets for"
msgstr "Blätter drucken"

#: FamilySheet/FamilySheet.py:823
msgid "Center person only"
msgstr "nur für Hauptperson"

#: FamilySheet/FamilySheet.py:824
msgid "Center person and descendants in side branches"
msgstr "für Hauptperson und Nachkommen in Nebenlinien"

#: FamilySheet/FamilySheet.py:825
msgid "Center person and all descendants"
msgstr "für Hauptperson und alle Nachkommen"

#: FamilySheet/FamilySheet.py:828
msgid "Use call name"
msgstr "Rufname"

#: FamilySheet/FamilySheet.py:830
msgid "Don't use call name"
msgstr "nicht verwenden"

#: FamilySheet/FamilySheet.py:831
msgid "Replace first name with call name"
msgstr "statt des Vornamens einsetzen"

#: FamilySheet/FamilySheet.py:832
msgid "Underline call name in first name / add call name to first name"
msgstr "im Vornamen unterstreichen / zum Vornamen hinzufügen"

#: FamilySheet/FamilySheet.py:835
msgid "Print placeholders for missing information"
msgstr "Platzhalter für fehlende Informationen drucken"

#: FamilySheet/FamilySheet.py:838
msgid "Include sources"
msgstr "Quellenangaben drucken"

#: FamilySheet/FamilySheet.py:841
msgid "Include notes"
msgstr "Notizen drucken"

#: FamilySheet/FamilySheet.py:855
msgid "The basic style used for the text display"
msgstr "Der grundlegende Stil, der für Text verwendet wird"

#: FamilySheet/FamilySheet.py:865
msgid "The style used for the page key on the top"
msgstr "Der Stil, der für den Schlüssel rechts oben verwendet wird"

#: FamilySheet/FamilySheet.py:874
msgid "The style used for names"
msgstr "Der Stil für Namen"

#: FamilySheet/FamilySheet.py:884
msgid "The style used for numbers"
msgstr "Der Stil für Nummern"

#: FamilySheet/FamilySheet.py:894
msgid "The style used for footnotes (notes and source references)"
msgstr "Der Stil für Fußnoten (Notizen und Quellenangaben)"
