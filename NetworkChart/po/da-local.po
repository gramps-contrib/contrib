#: NetworkChart/NetworkChart.gpr.py:23
msgid "Network Chart"
msgstr "Netværksdiagram"

#: NetworkChart/NetworkChart.gpr.py:34
msgid "Generates a family network chart."
msgstr "Danner et familie netværksdiagram."

#: NetworkChart/NetworkChart.py:133
msgid "birth abbreviation|b."
msgstr "f."

#: NetworkChart/NetworkChart.py:134
msgid "death abbreviation|d."
msgstr "d."

#: NetworkChart/NetworkChart.py:135
msgid "marriage abbreviation|m."
msgstr "g."

#: NetworkChart/NetworkChart.py:136
msgid "Unknown"
msgstr "Ukendt"

#: NetworkChart/NetworkChart.py:244
msgid "File exists. Overwrite?"
msgstr "Filen findes. Overskriv?"

#: NetworkChart/NetworkChart.py:245
msgid "Cancel"
msgstr "Afbryd"

#: NetworkChart/NetworkChart.py:245
msgid "Yes"
msgstr "Ja"

#: NetworkChart/NetworkChart.py:898
msgid "created"
msgstr "dannet"

#: NetworkChart/NetworkChart.py:899 NetworkChart/NetworkChart.py:902
msgid "last_written"
msgstr "senest_skrevet"

#: NetworkChart/NetworkChart.py:912
msgid "Main"
msgstr "Hoved"

#: NetworkChart/NetworkChart.py:914
msgid "Graph Style"
msgstr "Diagramstil"

#: NetworkChart/NetworkChart.py:916
msgid "Orthogonal (right angles in connectors)"
msgstr "Ortogonal (rette vinkler i forbindelserne)"

#: NetworkChart/NetworkChart.py:917
msgid "Straight (no right angles in connectors)"
msgstr "Direkte (ingen rette vinkler i forbindelserne)"

#: NetworkChart/NetworkChart.py:918
msgid "Curved (curved and straight connectors)"
msgstr "Buet (buede og lige forbindelser)"

#: NetworkChart/NetworkChart.py:920
msgid "Default (Orthogonal)"
msgstr "Standard (Ortogonal)"

#: NetworkChart/NetworkChart.py:925
msgid "Select the graph line connector format."
msgstr "Vælg diagrammets linjeforbindelse format."

#: NetworkChart/NetworkChart.py:928
msgid "Graph Direction"
msgstr "Diagrammets retning"

#: NetworkChart/NetworkChart.py:930
msgid "Top to Bottom"
msgstr "Top til bund"

#: NetworkChart/NetworkChart.py:930
msgid "Left to Right"
msgstr "Venstre til højre"

#: NetworkChart/NetworkChart.py:931
msgid "Bottom to Top"
msgstr "Bund til top"

#: NetworkChart/NetworkChart.py:931
msgid "Right to Left"
msgstr "Højre til vnstre"

#: NetworkChart/NetworkChart.py:933
msgid "Default (Top to Bottom)"
msgstr "Standard (top til bund)"

#: NetworkChart/NetworkChart.py:936
msgid "Select the graph direction."
msgstr "Vælg diagrammet retning."

#: NetworkChart/NetworkChart.py:939
msgid "URL Style"
msgstr "URL stil"

#: NetworkChart/NetworkChart.py:941
msgid "Include URLs from database."
msgstr "Medtag URLs fra databasen."

#: NetworkChart/NetworkChart.py:942
msgid "Dynamic URL = Prefix + GrampID + Suffix"
msgstr "Dynamisk URL = Prefix + GrampsID + Suffix"

#: NetworkChart/NetworkChart.py:943
msgid "Static URL = Prefix"
msgstr "Statisk URL = Prefix"

#: NetworkChart/NetworkChart.py:944
msgid "Don't include URLs"
msgstr "Medtag ikke URL"

#: NetworkChart/NetworkChart.py:945
msgid "Default (include)"
msgstr "Standard (medtag)"

#: NetworkChart/NetworkChart.py:948
msgid "Select URL style."
msgstr "Vælg URL stil."

#: NetworkChart/NetworkChart.py:951
msgid "URL Prefix,Suffix"
msgstr "URL Prefix,Suffix"

#: NetworkChart/NetworkChart.py:952
msgid ""
"Enter the Prefix,Suffix (use comma)\n"
"for dynamically generated URLs.\n"
"URL = Prefix + GrampsID + Suffix."
msgstr ""
"Angic Prefix,Suffix (benyt komma)\n"
"til dynamisk dannede URLs.\n"
"URL = Prefix + GrampsID + Suffix."

#: NetworkChart/NetworkChart.py:957
msgid "Enter Font Name"
msgstr "Angiv Fontnavn"

#: NetworkChart/NetworkChart.py:959
msgid ""
"Enter the primary font style for the chart.\n"
"Must already be installed.\n"
"White Rabbit can be obtained from\n"
"https://www.fontsquirrel.com/fonts/white-rabbit"
msgstr ""
"Angiv den primære font til diagrammet.\n"
"Skal allerede være installeret.\n"
"White Rabbit kan fås fra\n"
"https://www.fontsquirrel.com/fonts/white-rabbit"

#: NetworkChart/NetworkChart.py:964
msgid "Spacing (inch)"
msgstr "Spatiering:(tommer)"

#: NetworkChart/NetworkChart.py:965
msgid ""
"Enter the seperation distance between \"Generations\" in inches (0.1-5.0)."
msgstr "Angiv mellemrummet mellem \"Generationer\" i tommer (0,1-5,0)."

#: NetworkChart/NetworkChart.py:969
msgid "Enter Chart Title"
msgstr "Angiv diagrammets titel"

#: NetworkChart/NetworkChart.py:971
msgid "Set the title of the chart."
msgstr "Angiv titlen på diagrammet."

#: NetworkChart/NetworkChart.py:974
msgid "File Type"
msgstr "Filtype"

#: NetworkChart/NetworkChart.py:976
msgid "Default (svg)"
msgstr "Standard (svg)"

#: NetworkChart/NetworkChart.py:979
msgid ""
"svg - Scalable Vector Graphics file.\n"
"pdf - Adobe Portable Document Format.\n"
msgstr ""
"svg - Scalable Vector Graphics file.\n"
"pdf - Adobe Portable Document Format.\n"

#: NetworkChart/NetworkChart.py:984
msgid "Folder"
msgstr "Mappe"

#: NetworkChart/NetworkChart.py:986
msgid "The destination folder for generated files."
msgstr "Mål stien for dannede filer."

#: NetworkChart/NetworkChart.py:990 NetworkChart/NetworkChart.py:1280
#: NetworkChart/NetworkChart.py:1288
msgid "network"
msgstr "netværk"

#: NetworkChart/NetworkChart.py:993
msgid "Filename"
msgstr "Filnavn"

#: NetworkChart/NetworkChart.py:995
msgid "The filename for the generated svg network chart."
msgstr "Filnavnet for det dannede svg netværksdiagram."

#: NetworkChart/NetworkChart.py:1002
msgid "Different node shapes/edges for gender."
msgstr "Forskellige noder/kanter for køn."

#: NetworkChart/NetworkChart.py:1003
msgid "Node differences for gender."
msgstr "Nodeforskelle for køn."

#: NetworkChart/NetworkChart.py:1006
msgid "Color"
msgstr "Farve"

#: NetworkChart/NetworkChart.py:1008
msgid "Male Background"
msgstr "Mandlig baggrund"

#: NetworkChart/NetworkChart.py:1009
msgid "RGB-color for male box background."
msgstr "RGB-farve til baggrund for mandlig boks ."

#: NetworkChart/NetworkChart.py:1012
msgid "Male Background Alpha"
msgstr "Mandlig baggrund Alpha"

#: NetworkChart/NetworkChart.py:1014
msgid "Alpha for Male box background (transparent=0, solid=255)."
msgstr "Alpha for mandlig boks baggrund (gennemsigtig=0, ugennemsigtig=255)."

#: NetworkChart/NetworkChart.py:1018
msgid "Male Box Edge"
msgstr "Kanten på mandlig boks"

#: NetworkChart/NetworkChart.py:1019
msgid "RGB-color for Male box edge."
msgstr "RGB-farve for mandlig boks kant."

#: NetworkChart/NetworkChart.py:1022
msgid "Female Background"
msgstr "Kvindelig baggrund"

#: NetworkChart/NetworkChart.py:1023
msgid "RGB-color for Female box background."
msgstr "RGB-farve til baggrund for kvindelig boks ."

# added:
#: NetworkChart/NetworkChart.py:1026
msgid "Female Background Alpha"
msgstr "Kvindelig baggrund Alpha"

#: NetworkChart/NetworkChart.py:1028
msgid "Alpha for Female box background (transparent=0, solid=255)."
msgstr "Alpha for kvindelig boks baggrund (gennemsigtig=0, ugennemsigtig=255)."

#: NetworkChart/NetworkChart.py:1033
msgid "Female Box Edge"
msgstr "Kanten på kvindelig boks"

#: NetworkChart/NetworkChart.py:1034
msgid "RGB-color for Female box edge."
msgstr "RGB-farve for kvindelig boks kant."

#: NetworkChart/NetworkChart.py:1037
msgid "Other Background"
msgstr "Anden baggrund"

#: NetworkChart/NetworkChart.py:1038
msgid "RGB-color for other box background."
msgstr "RGB-farve for anden boks baggrund."

#: NetworkChart/NetworkChart.py:1041
msgid "Other Background Alpha"
msgstr "Anden baggrund Alpha"

#: NetworkChart/NetworkChart.py:1043
msgid "Alpha for Other box background (transparent=0, solid=255)."
msgstr "Alpha for anden boks baggrund (gennemsigtig=0, ugennemsigtig=255)."

#: NetworkChart/NetworkChart.py:1047
msgid "Family Connector Line"
msgstr "Familie forbindelseslinje"

#: NetworkChart/NetworkChart.py:1048
msgid "RGB-color for family connector line."
msgstr "RGB-farve for familie forbindelseslinje."

#: NetworkChart/NetworkChart.py:1051
msgid "Marriage Connector Line"
msgstr "Ægteskab forbindelseslinje"

#: NetworkChart/NetworkChart.py:1052
msgid "RGB-color for marriage connector line."
msgstr "RGB-farve for ægteskabs forbindelseslinje."

#: NetworkChart/NetworkChart.py:1055
msgid "Highlight Connector Line"
msgstr "Fremhæv forbindelseslinje"

#: NetworkChart/NetworkChart.py:1056
msgid ""
"RGB-color for the highlighted path between two individuals.  If the graph."
"inverts, reverse the order to change."
msgstr ""
"RGB-farve for den fremhævede sti mellem to personer.  Hvis graph.inverts, "
"vend ordenen der skal skifte."

#: NetworkChart/NetworkChart.py:1061
msgid "Trim Box Edge"
msgstr "Trim bokskanter"

#: NetworkChart/NetworkChart.py:1062
msgid "RGB-color for box edge that has been trimmed."
msgstr "RGB-farve for bokse, der er trimmet."

#: NetworkChart/NetworkChart.py:1066
msgid "Remove color background on all nodes."
msgstr "Fjern farvebaggrund for alle noder."

#: NetworkChart/NetworkChart.py:1068
msgid "Removes color background for individuals (nodes) on chart."
msgstr "Fjerner farvebaggrund for personer (noder) på digrammet."

#: NetworkChart/NetworkChart.py:1071
msgid "Privacy"
msgstr "Persondatabeskyttelse"

#: NetworkChart/NetworkChart.py:1073
msgid ""
"Enter year to start\n"
"rounding birthday\n"
"to year only"
msgstr ""
"Angiv året for begyndelse\n"
"at afrunde fødselsdage\n"
"til kun år"

#: NetworkChart/NetworkChart.py:1076
msgid ""
"Birthdays after this year are represented by the year only.  Invalid entries "
"will be ignored."
msgstr ""
"Fødselsdage efter dette år ses kun som år.  Ugyldige poster vil blive "
"ignorerede."

#: NetworkChart/NetworkChart.py:1080
msgid "Round birthday to year after year entered (above)."
msgstr "Afrund fødselsdag til år efter det angivne år (ovenfor)."

#: NetworkChart/NetworkChart.py:1082
msgid "Represent birthday by year only."
msgstr "Vis kun fødselsdag som år."

#: NetworkChart/NetworkChart.py:1086
msgid ""
"Enter year to start\n"
"rounding marriage\n"
"date to year only"
msgstr ""
"Angiv året der begynder\n"
"afrunding af bryllups-\n"
"dato til kun år"

#: NetworkChart/NetworkChart.py:1087
msgid ""
"Marriages after this year are represented bythe year only. Invalid entries "
"will be ignored."
msgstr ""
"Ægteskaber efter dette år ses kun som år. Ugyldige poster vil blive "
"ignorerede."

#: NetworkChart/NetworkChart.py:1092
msgid "Round marriage to year after year entered (above)."
msgstr "Afrund bryllupsdato til år efter det angivne år (ovenfor)."

#: NetworkChart/NetworkChart.py:1094
msgid "Represent marriage by year only."
msgstr "Vis bryllupsdato udelukkende som år."

#: NetworkChart/NetworkChart.py:1099
msgid ""
"Attempts to remove middle names.  Only works for entries with birth year in "
"the record."
msgstr ""
"Forsøg på at fjerne mellemvan.  Virker kun for poster med fødselsdato i "
"posten."

#: NetworkChart/NetworkChart.py:1107
msgid "Remove middle names."
msgstr "Fjern mellemnavne."

#: NetworkChart/NetworkChart.py:1112
msgid "Allow inclusion of private records in chart."
msgstr "Tillad at private poster medtages i diagrammet."

#: NetworkChart/NetworkChart.py:1117
msgid "Trim"
msgstr "Trim"

#: NetworkChart/NetworkChart.py:1119
msgid "Trim descendants"
msgstr "Trim efterkommere"

#: NetworkChart/NetworkChart.py:1121
msgid "All descendants (children) of selected individuals will not be shown."
msgstr "Alle efterkommere (børn) af valgter personer vil ikke blive vist."

#: NetworkChart/NetworkChart.py:1127
msgid "Enable trimming of descendants."
msgstr "Tillad at efterkommere trimmes."

#: NetworkChart/NetworkChart.py:1129
msgid ""
"You must enter valid person(s) before \n"
"enabling trimming of descendants from tree."
msgstr ""
"Du skal angive gyldige person(er) tnden\n"
"du tillader at efterkommere trimmes fra træet."

#: NetworkChart/NetworkChart.py:1134
msgid "Trim ancestors"
msgstr "Trim aner"

#: NetworkChart/NetworkChart.py:1135
msgid "All ancestors (parents) of selected individuals will not be shown."
msgstr "Alle aner (forældre) til valgte personer vil ikke blive vist."

#: NetworkChart/NetworkChart.py:1141
msgid "Enable trimming of ancestors."
msgstr "Tillad at aner trimmes."

#: NetworkChart/NetworkChart.py:1143
msgid "Enable trimming of ancestors from tree."
msgstr "Tillad at aner trimmes fra træet."

#: NetworkChart/NetworkChart.py:1147
msgid "Enable trim groups."
msgstr "Tillad trmning af grupper."

#: NetworkChart/NetworkChart.py:1149
msgid ""
"Remove groups less than Min Group Size.  Automatic\n"
"for databases with more than 1500 individuals."
msgstr ""
"Fjern grupper mindre en Min gruppestørrelse.  Automatisk\n"
"i databaser med mere end 1500 personer."

#: NetworkChart/NetworkChart.py:1154
msgid "Min Group Size"
msgstr "Min gruppestørrelse"

#: NetworkChart/NetworkChart.py:1156
msgid ""
"Enter the minimum size group to display.\n"
"Value may be 2 or greater."
msgstr ""
"Angiv den mindste gruppestørrelse der vises\n"
"Værdien kan være 2 eller større."

#: NetworkChart/NetworkChart.py:1161
msgid "Highlight"
msgstr "Fremhæv"

#: NetworkChart/NetworkChart.py:1164
msgid ""
"Select Start and\n"
"End Individuals in\n"
"displayed path(s).\n"
"Select two"
msgstr ""
"Vælg start og\n"
"slutpersoner i\n"
"den viste sti.\n"
"Vælg to"

#: NetworkChart/NetworkChart.py:1167
msgid ""
"Starting and ending person for displayed path(s).\n"
"Select two people"
msgstr ""
"Start og slutpersoner for viste sti(er).\n"
"Vælg to personer"

#: NetworkChart/NetworkChart.py:1171
msgid "Highlight path(s)"
msgstr "Fremhæv sti(er)"

#: NetworkChart/NetworkChart.py:1174 NetworkChart/NetworkChart.py:1188
msgid "None"
msgstr "Ingen"

#: NetworkChart/NetworkChart.py:1174 NetworkChart/NetworkChart.py:1188
msgid "Direct"
msgstr "Direkte"

#: NetworkChart/NetworkChart.py:1174 NetworkChart/NetworkChart.py:1188
msgid "Any"
msgstr "Enhver"

#: NetworkChart/NetworkChart.py:1176
msgid "Default (None)"
msgstr "Standard (Ingen)"

#: NetworkChart/NetworkChart.py:1181
msgid ""
"None - Don't highlight paths.\n"
"Direct - Highlight direct descendant/ancestor paths.\n"
"Any - Highlight any path(s) including direct or indirect."
msgstr ""
"Ingen - undlad at fremhæve stier.\n"
"Direkte - fremhæv direkte efterkomme-/ane- stier.\n"
"Enhver - fremhæv alle stier både direkte og indirekte."

#: NetworkChart/NetworkChart.py:1186
msgid "Show only path(s)"
msgstr "Vis kun sti(er)"

#: NetworkChart/NetworkChart.py:1189
msgid "Default (none)"
msgstr "Standard (Ingen)"

#: NetworkChart/NetworkChart.py:1193
msgid ""
"None - Don't show paths only.\n"
"Direct - Show only direct descendant/ancestor paths.\n"
"Any - Show only path(s) direct or indirect."
msgstr ""
"Ingen - Vis ikke kun stier.\n"
"Direkte - vis kun direkte efterkommer-/ane- sti.\n"
"Enhver - vis kun sti(er) direkte eller indirekte."

#: NetworkChart/NetworkChart.py:1198
msgid "Center Person"
msgstr "Hovedperson"

#: NetworkChart/NetworkChart.py:1200
msgid "Select person at center of selection radius in graph."
msgstr "Vælg personen i centrum for udvalgsradius i diagrammet.)"

#: NetworkChart/NetworkChart.py:1203
msgid ""
"Max connections\n"
"from center"
msgstr ""
"Max forbindelser\n"
"fra centrum"

#: NetworkChart/NetworkChart.py:1206
msgid ""
"Enter the maximum number of connections allowed from\n"
"the center person.  Value may be 1 or greater."
msgstr ""
"Angiv det højeste antal tilladte forbindelser fra\n"
"hovedpersonen.  Værdien kan være 1 eller højere."

#: NetworkChart/NetworkChart.py:1211
msgid "Limit graph to max connections from center."
msgstr "Begræns diagrammet til max forbindelser fra centrum."

#: NetworkChart/NetworkChart.py:1213
msgid "Display up to max connections from central person."
msgstr "Vis op til max forbindelser fra hovedpersonen."

#: NetworkChart/NetworkChart.py:1217
msgid "Highlight central person in graph."
msgstr "Fremhæv hoverpersonen i diagrammet."

#: NetworkChart/NetworkChart.py:1219
msgid "Add yellow color background to central person."
msgstr "Tilføj gul baggrund til hovedpersonen."

#: NetworkChart/NetworkChart.py:1223
msgid "Config"
msgstr "Konfig"

#: NetworkChart/NetworkChart.py:1227
msgid "Database"
msgstr "Database"

#: NetworkChart/NetworkChart.py:1234
msgid "Use database handle instead of GrampID id in URLs."
msgstr "Benyt databasehåndtaget istede for GrampsID i URLs."

#: NetworkChart/NetworkChart.py:1236
msgid ""
"Use database handle instead of the gramps id for URLs.\n"
"The handle should never change whereas gramps ids can."
msgstr ""
"Benyt databasehåndtaget i stedet for GrampsID i URLS.\n"
"Håndtaget burde aldrig ændres, hvorimod GrampsID kan."

#: NetworkChart/NetworkChart.py:1240
msgid "Confirm overwrite file."
msgstr "Bekræft overskriv fil."

#: NetworkChart/NetworkChart.py:1243
msgid "Enable/disable confirmation of file overwrite."
msgstr "Aktiver/deaktiver bekræftelse af overskrivning af fil."
