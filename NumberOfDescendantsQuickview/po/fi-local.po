#: NumberOfDescendantsQuickview.gpr.py:3
msgid "Number of descendants"
msgstr "Jälkeläisten määrä"

#: NumberOfDescendantsQuickview.gpr.py:4
msgid "Shows the number of descendants of the current person"
msgstr "Näyttää nykyisen henkilön jälkeläisten lukumäärän"

#: NumberOfDescendantsQuickview.py:62
#, python-format
msgid "Number of %s's descendants"
msgstr "Henkilön %s jälkeläisten lukumäärä"

#: NumberOfDescendantsQuickview.py:87 NumberOfDescendantsQuickview.py:94
msgid "Generation"
msgstr "Sukupolvi"

#: NumberOfDescendantsQuickview.py:88 NumberOfDescendantsQuickview.py:95
#: NumberOfDescendantsQuickview.py:111 NumberOfDescendantsQuickview.py:115
msgid "Total"
msgstr "Yhteensä"

#: NumberOfDescendantsQuickview.py:89
msgid "Seen"
msgstr "Nähnyt"

#: NumberOfDescendantsQuickview.py:90
msgid "Outlived"
msgstr "Kuollut"

#: NumberOfDescendantsQuickview.py:91 NumberOfDescendantsQuickview.py:96
msgid "Now alive"
msgstr "Elossa"

#: NumberOfDescendantsQuickview.py:122
#, python-format
msgid "Seen = number of descendants whose birth %s has lived to see"
msgstr "Nähnyt = jälkeläisten määrä, joiden syntymän %s on eläessään nähnyt"

#: NumberOfDescendantsQuickview.py:124
#, python-format
msgid "Outlived = number of descendants who died while %s was still alive"
msgstr ""
"Kuollut = jälkeläisten lukumäärä, jotka kuolivat, kun %s on ollut vielä "
"elossa"
