#: FamilyTree/FamilyTree.gpr.py:3
msgid "Family Tree"
msgstr "Árvore de família"

#: FamilyTree/FamilyTree.gpr.py:4
msgid "Produces a graphical family tree."
msgstr "Produz uma árvore de família."

#: FamilyTree/FamilyTree.py:102
#, python-format
msgid "Family Tree for %s"
msgstr "Árvore de família de %s"

#: FamilyTree/FamilyTree.py:152
msgid "Paper too small"
msgstr "Papel muito pequeno"

#: FamilyTree/FamilyTree.py:153
msgid "Some elements may not appear or be badly rendered."
msgstr "Alguns elementos podem não aparecer ou ser mal desenhados."

#: FamilyTree/FamilyTree.py:672 FamilyTree/FamilyTree.py:677
msgid "Unknown"
msgstr "Desconhecido"

#: FamilyTree/FamilyTree.py:679
#, python-format
msgid "%(father)s and %(mother)s"
msgstr "%(father)s e %(mother)s"

#: FamilyTree/FamilyTree.py:686
msgid "Anonymous"
msgstr "Anónimo"

#: FamilyTree/FamilyTree.py:913
msgid "born"
msgstr "nascido"

#: FamilyTree/FamilyTree.py:915
msgid "baptised"
msgstr "baptizado"

#: FamilyTree/FamilyTree.py:917
msgid "died"
msgstr "falecido"

#: FamilyTree/FamilyTree.py:919
msgid "buried"
msgstr "sepultado"

#: FamilyTree/FamilyTree.py:921
msgid "cremated"
msgstr "cremado"

#: FamilyTree/FamilyTree.py:923
msgid "married"
msgstr "casado"

#: FamilyTree/FamilyTree.py:925
msgid "divorced"
msgstr "divorciado"

#: FamilyTree/FamilyTree.py:927
msgid "resident"
msgstr "residente"

#: FamilyTree/FamilyTree.py:934
#, python-format
msgid "on %(ymd_date)s"
msgstr "a %(ymd_date)s"

#: FamilyTree/FamilyTree.py:936
#, python-format
msgid "in %(ym_date)s"
msgstr "em %(ym_date)s"

#: FamilyTree/FamilyTree.py:938
#, python-format
msgid "in %(y_date)s"
msgstr "em %(y_date)s"

#: FamilyTree/FamilyTree.py:942
#, python-format
msgid "on %(placeholder)s"
msgstr "em %(placeholder)s"

#: FamilyTree/FamilyTree.py:945
#, python-format
msgid "on %(placeholder)s (%(partial)s)"
msgstr "em %(placeholder)s (%(partial)s)"

#: FamilyTree/FamilyTree.py:962
#, python-format
msgid "in %(place)s"
msgstr "em %(place)s"

#: FamilyTree/FamilyTree.py:973
#, python-format
msgid "(%(description)s)"
msgstr "(%(description)s)"

#: FamilyTree/FamilyTree.py:1043
msgid "Tree Options"
msgstr "Opções da árvore"

#: FamilyTree/FamilyTree.py:1045
msgid "Center Family"
msgstr "Família central"

#: FamilyTree/FamilyTree.py:1046
msgid "The center family for the tree"
msgstr "A família central da árvore"

#: FamilyTree/FamilyTree.py:1049
msgid "Ancestor Generations"
msgstr "Gerações ascendentes"

#: FamilyTree/FamilyTree.py:1050
msgid "The number of ancestor generations to include in the tree"
msgstr "O número de gerações ascendentes a incluir na árvore"

#: FamilyTree/FamilyTree.py:1053
msgid "Descendant Generations"
msgstr "Gerações descendentes"

#: FamilyTree/FamilyTree.py:1054
msgid "The number of descendant generations to include in the tree"
msgstr "O número de gerações descendentes a incluir na árvore"

#: FamilyTree/FamilyTree.py:1057
msgid "Kekule number of husband"
msgstr "Número Kekule de marido"

#: FamilyTree/FamilyTree.py:1058
msgid ""
"The Kekule number of the husband (central family). Set 0 to not show Kekule "
"numbers"
msgstr ""
"O número Kekule do marido (família central). Defina como 0 para ocultar "
"números Kekule"

#: FamilyTree/FamilyTree.py:1061
msgid "Scale to fit on a single page"
msgstr "Escalar para caber numa só página"

#: FamilyTree/FamilyTree.py:1062
msgid "Whether to scale to fit on a single page."
msgstr "Se deve escalar para caber tudo numa só página."

#: FamilyTree/FamilyTree.py:1065
msgid "Color"
msgstr "Cor"

#: FamilyTree/FamilyTree.py:1067
msgid "No color"
msgstr "Sem cor"

#: FamilyTree/FamilyTree.py:1068
msgid "Generations"
msgstr "Gerações"

#: FamilyTree/FamilyTree.py:1069
msgid "First generation"
msgstr "Primeira geração"

#: FamilyTree/FamilyTree.py:1070
msgid "Second generation"
msgstr "Segunda geração"

#: FamilyTree/FamilyTree.py:1071
msgid "Third generation"
msgstr "Terceira geração"

#: FamilyTree/FamilyTree.py:1072
msgid "Male line"
msgstr "Linha masculina"

#: FamilyTree/FamilyTree.py:1073
msgid "Male line and illegitimate children"
msgstr "Linha masculina e filhos ilegítimos"

#: FamilyTree/FamilyTree.py:1074
msgid "Female line"
msgstr "Linha feminina"

#: FamilyTree/FamilyTree.py:1077
msgid "Shuffle colors"
msgstr "Baralhar cores"

#: FamilyTree/FamilyTree.py:1078
msgid "Whether to shuffle colors or order them in rainbow fashion."
msgstr "Se deve baralhar cores ou ordená-las como arco-íris"

#: FamilyTree/FamilyTree.py:1081
msgid "Content"
msgstr "Conteúdo"

#: FamilyTree/FamilyTree.py:1083
msgid "Use call name"
msgstr "Usar nome usual"

#: FamilyTree/FamilyTree.py:1085
msgid "Don't use call name"
msgstr "Não usar nome usual"

#: FamilyTree/FamilyTree.py:1086
msgid "Replace first name with call name"
msgstr "Substituir nome próprio pelo nome usual"

#: FamilyTree/FamilyTree.py:1087
msgid "Underline call name in first name / add call name to first name"
msgstr "Sublinhar nome usual no nome/adicionar nome usual ao nome"

#: FamilyTree/FamilyTree.py:1093
msgid "Include Occupation"
msgstr "Incluir profissão"

#: FamilyTree/FamilyTree.py:1096
msgid "Include Notes"
msgstr "Incluir notas"

#: FamilyTree/FamilyTree.py:1099
msgid "Include Residence"
msgstr "Incluir residência"

#: FamilyTree/FamilyTree.py:1102
msgid "Print event data (dead person)"
msgstr "Imprimir dados de eventos (falecidos)"

#: FamilyTree/FamilyTree.py:1104 FamilyTree/FamilyTree.py:1112
msgid "None"
msgstr "Nada"

#: FamilyTree/FamilyTree.py:1105 FamilyTree/FamilyTree.py:1113
msgid "Year only"
msgstr "Só o ano"

#: FamilyTree/FamilyTree.py:1106 FamilyTree/FamilyTree.py:1114
msgid "Full date"
msgstr "Data completa"

#: FamilyTree/FamilyTree.py:1107 FamilyTree/FamilyTree.py:1115
msgid "Full date and place"
msgstr "Data completa e local"

#: FamilyTree/FamilyTree.py:1110
msgid "Print event data (living person)"
msgstr "Imprimir dados de eventos (vivos)"

#: FamilyTree/FamilyTree.py:1118
msgid "Fall back to baptism if birth event missing"
msgstr "Usar baptismo se o nascimento estiver em falta"

#: FamilyTree/FamilyTree.py:1121
msgid "Fall back to burial or cremation if death event missing"
msgstr "Usar funeral ou cremação se o óbito estiver em falta"

#: FamilyTree/FamilyTree.py:1124
msgid "Protect private items"
msgstr "Proteger itens privados"

#: FamilyTree/FamilyTree.py:1129
msgid "Print fields for missing information"
msgstr "Imprimir campos para informação em falta"

#: FamilyTree/FamilyTree.py:1130
msgid "Whether to include fields for missing information."
msgstr "Se deve incluir campos para informação em falta."

#: FamilyTree/FamilyTree.py:1133
msgid "Include event description"
msgstr "Incluir descrição de evento"

#: FamilyTree/FamilyTree.py:1136
msgid "Text Options"
msgstr "Opções de texto"

#: FamilyTree/FamilyTree.py:1138
msgid "Title text"
msgstr "Texto do título"

#: FamilyTree/FamilyTree.py:1141
msgid "Footer text"
msgstr "Texto do rodapé"

#: FamilyTree/FamilyTree.py:1156
msgid "The style used for the title."
msgstr "O estilo usado para o título."

#: FamilyTree/FamilyTree.py:1164
msgid "The style used for names."
msgstr "O estilo usado para nomes."

#: FamilyTree/FamilyTree.py:1172
msgid "The style used for data (birth, death, marriage, divorce)."
msgstr "O estilo usado para dados (nascimento, óbito, casamento, divórcio)."

#: FamilyTree/FamilyTree.py:1182
msgid "The style used for the footer."
msgstr "O estilo usado para o rodapé."
