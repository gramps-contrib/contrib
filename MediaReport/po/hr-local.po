#: MediaReport/media_report.gpr.py:23
msgid "Media Report"
msgstr "Izvještaj o medijima"

#: MediaReport/media_report.gpr.py:24
msgid "Generates report including images, image data and notes."
msgstr "Izrađuje izvještaj koji uključuje slike, podatke slika i zabilješke."

#: MediaReport/media_report.py:106 MediaReport/media_report.py:112
#: MediaReport/media_report.py:119 MediaReport/media_report.py:125
msgid "INFO"
msgstr "INFORMACIJE"

#: MediaReport/media_report.py:106 MediaReport/media_report.py:119
msgid "You have to select an image to generate this report."
msgstr "Za stvaranje ovog izvještaja moraš odabrati jednu sliku."

#: MediaReport/media_report.py:112
msgid ""
"You have to select a custom note or uncheck the option 'include custom note' "
"to generate this report."
msgstr ""
"Za stvaranje ovog izvještaja moraš odabrati jednu prilagođenu zabilješku ili "
"odznači opciju „Uključi prilagođenu zabilješku”."

#: MediaReport/media_report.py:125
msgid "This report only supports PDF as output file format."
msgstr "Ovaj izvještaj podržava samo PDF kao izlazni format."

#: MediaReport/media_report.py:229
msgid "General:"
msgstr "Opće:"

#: MediaReport/media_report.py:235
msgid "Description:"
msgstr "Opis:"

#: MediaReport/media_report.py:250
msgid "Image type:"
msgstr "Vrsta slike:"

#: MediaReport/media_report.py:342
msgid "File does not exist"
msgstr "Datoteka ne postoji"

#: MediaReport/media_report.py:343
msgid "Could not add photo to page"
msgstr "Nije moguće sliku dodati na stranicu"

#: MediaReport/media_report.py:344
#, python-format
msgid "%(str1)s: %(str2)s"
msgstr "%(str1)s: %(str2)s"

#: MediaReport/media_report.py:426
msgid "Heading"
msgstr "Naslov"

#: MediaReport/media_report.py:427 MediaReport/media_report.py:431
#: MediaReport/media_report.py:435 MediaReport/media_report.py:439
#: MediaReport/media_report.py:444 MediaReport/media_report.py:448
#: MediaReport/media_report.py:453 MediaReport/media_report.py:458
msgid "Report Options"
msgstr "Opcije za izvještaj"

#: MediaReport/media_report.py:429
msgid "Media"
msgstr "Mediji"

#: MediaReport/media_report.py:430
msgid "Select a media file for this report"
msgstr "Odaberi datoteku medija za ovaj izvještaj"

#: MediaReport/media_report.py:433
msgid "Custom note"
msgstr "Prilagođena zabilješka"

#: MediaReport/media_report.py:434
msgid "Select a note for this report"
msgstr "Uključi zabilješku za ovaj izvještaj"

#: MediaReport/media_report.py:437
msgid "Include custom note"
msgstr "Uključi prilagođenu zabilješku"

#: MediaReport/media_report.py:438
msgid "The custom note will be included"
msgstr "Prilagođena zabilješka će se uključiti u izvještaj"

#: MediaReport/media_report.py:442
msgid "Include referenced people"
msgstr "Uključi osobe povezane s medijem"

#: MediaReport/media_report.py:443
msgid "Referenced people will be included"
msgstr "Osobe povezane s medijem će se uključiti u izvještaj"

#: MediaReport/media_report.py:446
msgid "Include media data"
msgstr "Uključi podatke medija"

#: MediaReport/media_report.py:447
msgid "Tags, notes and attributes will be included"
msgstr "Etikete, zabilješke i svojstva će se uključiti u izvještaj"

#: MediaReport/media_report.py:450
msgid "Media width"
msgstr "Širina medija"

#: MediaReport/media_report.py:451
#, python-format
msgid "Maximum media width in % of available page width."
msgstr "Maksimalna širina medija u postocima dostupne širine stranice."

#: MediaReport/media_report.py:455
msgid "Media height"
msgstr "Visina medija"

#: MediaReport/media_report.py:456
#, python-format
msgid "Maximum media height in % of available page height."
msgstr "Maksimalna visina medija u postocima dostupne visine stranice."
