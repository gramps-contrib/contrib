msgid "Set Privacy Tool"
msgstr "Ställ in Sekretessverktyg"

#: SetPrivacyTool/SetPrivacyTool.gpr.py:24
msgid "Set all objects of the last <number> of years private."
msgstr "Ange alla objekt för de senaste <number> åren som privata."

#: SetPrivacyTool/SetPrivacyTool.py:89
#, python-format
msgid "Set private: %d %s\n"
msgstr "Ange privata: %d %s\n"

#: SetPrivacyTool/SetPrivacyTool.py:91
#, python-format
msgid "Not private: %d %s\n"
msgstr "Ej privata: %d %s\n"

#: SetPrivacyTool/SetPrivacyTool.py:103
msgid "Set persons private.."
msgstr "Ange personer privata.."

#: SetPrivacyTool/SetPrivacyTool.py:152
msgid "Set events private.."
msgstr "Ange händelser privata.."

#: SetPrivacyTool/SetPrivacyTool.py:190
msgid "Set media private.."
msgstr "Ange media privata.."

#: SetPrivacyTool/SetPrivacyTool.py:232
msgid "Persons"
msgstr "Personer"

#: SetPrivacyTool/SetPrivacyTool.py:233 SetPrivacyTool/SetPrivacyTool.py:237
#: SetPrivacyTool/SetPrivacyTool.py:244 SetPrivacyTool/SetPrivacyTool.py:248
#: SetPrivacyTool/SetPrivacyTool.py:255 SetPrivacyTool/SetPrivacyTool.py:259
#: SetPrivacyTool/SetPrivacyTool.py:268 SetPrivacyTool/SetPrivacyTool.py:274
msgid "Option"
msgstr "Alternativ"

#: SetPrivacyTool/SetPrivacyTool.py:235
msgid "Person Filter"
msgstr "Personfilter"

#: SetPrivacyTool/SetPrivacyTool.py:236
msgid "Select filter to restrict people"
msgstr "Välj ett filter för att begränsa personer"

#: SetPrivacyTool/SetPrivacyTool.py:239 SetPrivacyTool/SetPrivacyTool.py:250
#: SetPrivacyTool/SetPrivacyTool.py:261
msgid "Entire Database"
msgstr "Hela databasen"

#: SetPrivacyTool/SetPrivacyTool.py:243
msgid "Events"
msgstr "Händelser"

#: SetPrivacyTool/SetPrivacyTool.py:246
msgid "Event Filter"
msgstr "Händelsefilter"

#: SetPrivacyTool/SetPrivacyTool.py:247
msgid "Select filter to restrict events"
msgstr "Välj filter för att begränsa händelser"

#: SetPrivacyTool/SetPrivacyTool.py:254
msgid "Media"
msgstr "Media"

#: SetPrivacyTool/SetPrivacyTool.py:257
msgid "Media Filter"
msgstr "Mediafilter"

#: SetPrivacyTool/SetPrivacyTool.py:258
msgid "Select filter to restrict medias"
msgstr "Välj filter för att begränsa media"

#: SetPrivacyTool/SetPrivacyTool.py:265
msgid "Always private if no date."
msgstr "Alltid privat om inget datum."

#: SetPrivacyTool/SetPrivacyTool.py:266
msgid "If checked, all objects without a date will also be set private."
msgstr "Om kontrollerade, alla objekt utan datum kommer också att anges privata."

#: SetPrivacyTool/SetPrivacyTool.py:270
msgid "Years"
msgstr "År"

#: SetPrivacyTool/SetPrivacyTool.py:271
msgid ""
"The time range in years from today you want to set objects private.\n"
"'0 years' = remove privacy from all objects."
msgstr ""
"Tidsintervallet i år från idag som du vill ange objekt privata.\n"
"'0 år' = ta bort sekretess från alla objekt."
